public class Fibonacci {
    public static int f(int n){

        if (n==0){
            return 0;
        }
        else if (n==1){
            return 1; 
        }
        else{
            return f(n-1) + f(n-2);
        }
    }
    public static void main(String[]args){
        int n= 6; 
        int result= f(n); 
        System.out.println(result);
    }
}
